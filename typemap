cpFloat                 T_NV
cpArbiter *             O_OBJECT
cpBody *                O_OBJ_BODY
cpConstraint *          O_OBJ_CONSTRAINT
cpShape *               O_OBJ_SHAPE
cpSpace *               O_OBJ_SPACE
cpVect                  O_OBJ_VECT
cpMat2x2                O_OBJ_MAT2X2
